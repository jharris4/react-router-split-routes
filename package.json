{
  "name": "react-router-split-routes",
  "version": "1.0.8",
  "description": "Split route loading components for react-router-routes/react-router-config",
  "keywords": [
    "react",
    "react-router",
    "code split",
    "routes"
  ],
  "license": "BSD-3-Clause",
  "author": {
    "name": "Jon Harris",
    "url": "https://github.com/jharris4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jharris4/react-router-split-routes.git"
  },
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "files": [
    "src",
    "lib",
    "es",
    "dist"
  ],
  "customBabelMap": {
    "base": {
      "presets": [
        "es2015"
      ],
      "plugins": [
        "transform-for-of-as-array",
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-react-jsx"
      ]
    },
    "overrides": {
      "shared": [
        {
          "envs": [
            "es",
            "min",
            "umd"
          ],
          "presets": [
            [
              "es2015",
              {
                "modules": false
              }
            ]
          ]
        }
      ]
    },
    "additions": {
      "shared": [
        {
          "envs": [
            "es",
            "lib"
          ],
          "plugins": [
            [
              "transform-runtime",
              {
                "polyfill": false
              }
            ]
          ]
        },
        {
          "envs": [
            "min",
            "umd"
          ],
          "plugins": [
            "babel-plugin-external-helpers"
          ]
        }
      ],
      "test": {
        "plugins": [
          "babel-plugin-rewire"
        ]
      },
      "min": {
        "plugins": [
          "transform-react-remove-prop-types"
        ]
      }
    }
  },
  "babel": {
    "presets": [
      "./babelrc.js"
    ]
  },
  "vendorize": [
    "react",
    "react-router",
    "react-router-routes"
  ],
  "globalize": {
    "react": "React",
    "react-router": "ReactRouter",
    "react-router-routes": "ReactRouterRoutes"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15.0.0 || ^16.0.0",
    "react-router": "^4.3.0",
    "react-router-routes": "^1.0.3"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-for-of-as-array": "^1.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.1.6",
    "react": "16.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.0",
    "rollup-analyzer": "^1.1.0",
    "rollup-analyzer-plugin": "^1.1.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "scripts": {
    "clean-lib": "rimraf lib",
    "clean-es": "rimraf es",
    "clean-bundle": "rimraf dist",
    "clean": "npm run clean-lib && npm run clean-es && npm run clean-bundle",
    "build-lib": "npm run clean-lib && cross-env BABEL_ENV=lib NODE_ENV=production babel src/ --out-dir lib",
    "build-es": "npm run clean-es && cross-env BABEL_ENV=es NODE_ENV=production babel src/ --out-dir es",
    "build": "npm run build-lib && npm run build-es",
    "bundle-umd": "cross-env BABEL_ENV=umd NODE_ENV=production rollup -c",
    "bundle-min": "cross-env BABEL_ENV=min NODE_ENV=production rollup -c",
    "bundle": "npm run clean-bundle && npm run bundle-umd && npm run bundle-min",
    "prepublishOnly": "npm run clean && npm run build && npm run bundle"
  }
}
